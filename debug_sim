:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x558e6174b800 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x558e61720490 .scope module, "debug_tb" "debug_tb" 3 5;
 .timescale -9 -12;
v0x558e617747e0_0 .var "a", 11 0;
v0x558e617748c0_0 .var "b", 11 0;
v0x558e61774990_0 .var "clk", 0 0;
v0x558e61774a90_0 .net "result", 11 0, v0x558e61774390_0;  1 drivers
v0x558e61774b60_0 .var "rst_n", 0 0;
v0x558e61774c50_0 .var "valid_in", 0 0;
v0x558e61774d20_0 .net "valid_out", 0 0, v0x558e61774640_0;  1 drivers
E_0x558e6174a4a0 .event posedge, v0x558e617742c0_0;
S_0x558e61720640 .scope module, "dut" "floating_point_multiplier_debug" 3 15, 4 3 0, S_0x558e61720490;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 12 "a";
    .port_info 3 /INPUT 12 "b";
    .port_info 4 /INPUT 1 "valid_in";
    .port_info 5 /OUTPUT 12 "result";
    .port_info 6 /OUTPUT 1 "valid_out";
v0x558e6175a3f0_0 .net "a", 11 0, v0x558e617747e0_0;  1 drivers
v0x558e617741e0_0 .net "b", 11 0, v0x558e617748c0_0;  1 drivers
v0x558e617742c0_0 .net "clk", 0 0, v0x558e61774990_0;  1 drivers
v0x558e61774390_0 .var "result", 11 0;
v0x558e61774470_0 .net "rst_n", 0 0, v0x558e61774b60_0;  1 drivers
v0x558e61774580_0 .net "valid_in", 0 0, v0x558e61774c50_0;  1 drivers
v0x558e61774640_0 .var "valid_out", 0 0;
E_0x558e6175cbb0/0 .event negedge, v0x558e61774470_0;
E_0x558e6175cbb0/1 .event posedge, v0x558e617742c0_0;
E_0x558e6175cbb0 .event/or E_0x558e6175cbb0/0, E_0x558e6175cbb0/1;
    .scope S_0x558e61720640;
T_0 ;
    %wait E_0x558e6175cbb0;
    %load/vec4 v0x558e61774470_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x558e61774390_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558e61774640_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x558e61774580_0;
    %assign/vec4 v0x558e61774640_0, 0;
    %load/vec4 v0x558e61774580_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x558e6175a3f0_0;
    %assign/vec4 v0x558e61774390_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558e61720490;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558e61774990_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x558e61774990_0;
    %inv;
    %store/vec4 v0x558e61774990_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x558e61720490;
T_2 ;
    %vpi_call/w 3 33 "$display", "Debug test starting..." {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558e61774b60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558e61774c50_0, 0, 1;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x558e617747e0_0, 0, 12;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x558e617748c0_0, 0, 12;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558e61774b60_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 992, 0, 12;
    %store/vec4 v0x558e617747e0_0, 0, 12;
    %pushi/vec4 1056, 0, 12;
    %store/vec4 v0x558e617748c0_0, 0, 12;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558e61774c50_0, 0, 1;
    %vpi_call/w 3 50 "$display", "Input: a=%b, b=%b, valid_in=%b", v0x558e617747e0_0, v0x558e617748c0_0, v0x558e61774c50_0 {0 0 0};
    %wait E_0x558e6174a4a0;
    %delay 1000, 0;
    %vpi_call/w 3 54 "$display", "After 1 cycle: result=%b, valid_out=%b (should have data)", v0x558e61774a90_0, v0x558e61774d20_0 {0 0 0};
    %load/vec4 v0x558e61774d20_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_2.2, 4;
    %load/vec4 v0x558e61774a90_0;
    %load/vec4 v0x558e617747e0_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_2.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %vpi_call/w 3 57 "$display", "*** PASS: Basic pipeline works! ***" {0 0 0};
    %jmp T_2.1;
T_2.0 ;
    %vpi_call/w 3 59 "$display", "*** FAIL: Expected valid_out=1, result=%b, got valid_out=%b, result=%b ***", v0x558e617747e0_0, v0x558e61774d20_0, v0x558e61774a90_0 {0 0 0};
T_2.1 ;
    %vpi_call/w 3 62 "$finish" {0 0 0};
    %end;
    .thread T_2;
